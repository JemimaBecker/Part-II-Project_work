#filter metadata to just Luminal Differentiated cells
library(dplyr)
WK_filter <- WK_Meta %>% select(X, Sample.. , Stage , cross , animal.ID , RIN, RNA.Qubit.concentration..ng.ul. , RIN.category) %>% filter(WK_Meta$Cell.type  == "Luminal Differentiated")

print(WK_filter)

#fitler grouped data to just Luminal differentiated
WK_grouped_LD_filter <- MouseMammaryGland_Cleaned_edgeR_fpkm_grouped %>% select(X, Luminal.Differentiated.Gestation.d14.5, Luminal.Differentiated.Gestation.d5.5, Luminal.Differentiated.Gestation.d9.5, Luminal.Differentiated.Involution.d1, Luminal.Differentiated.Involution.d14, Luminal.Differentiated.Involution.d6, Luminal.Differentiated.Lactation.d10, Luminal.Differentiated.Lactation.d15, Luminal.Differentiated.Lactation.d5, Luminal.Differentiated.Nulliparous.d0)
names(WK_grouped_LD_filter)[1] <- "gene"

#h19 expression in differentiated luminal cells
H19_grouped_LD_filter1 <- MouseMammaryGland_Cleaned_edgeR_fpkm_grouped %>% select(X, Luminal.Differentiated.Gestation.d14.5, Luminal.Differentiated.Gestation.d5.5, Luminal.Differentiated.Gestation.d9.5, Luminal.Differentiated.Involution.d1, Luminal.Differentiated.Involution.d14, Luminal.Differentiated.Involution.d6, Luminal.Differentiated.Lactation.d10, Luminal.Differentiated.Lactation.d15, Luminal.Differentiated.Lactation.d5, Luminal.Differentiated.Nulliparous.d0) %>% filter(MouseMammaryGland_Cleaned_edgeR_fpkm_grouped$X == "ENSMUSG00000000031")
print(H19_grouped_LD_filter)
H19_grouped_LD_filter2 <- t(H19_grouped_LD_filter1) 

#H19 in all tissues ENSMUSG00000000031 - what other variants of H19 are there?
H19_grouped_all <- MouseMammaryGland_Cleaned_edgeR_fpkm_grouped %>% filter(MouseMammaryGland_Cleaned_edgeR_fpkm_grouped$X == "ENSMUSG00000000031")
H19_grouped_all2 <- t(H19_grouped_all)
